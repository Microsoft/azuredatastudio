{
    "name": "book",
    "displayName": "book",
    "description": "Support for Jupyter Books",
    "version": "0.0.1",
    "publisher": "Microsoft",
    "engines": {
        "vscode": "*",
        "azdata": "*"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "views": {
            "explorer": [
                {
                    "id": "bookTreeView",
                    "name": "Books"
                }
            ]
        },
        "commands": [
            {
                "command": "bookTreeView.refreshEntry",
                "title": "Refresh",
                "icon": {
                    "light": "resources/light/refresh.svg",
                    "dark": "resources/dark/refresh.svg"
                }
            },
            {
                "command": "bookTreeView.addEntry",
                "title": "Add"
            },
            {
                "command": "bookTreeView.editEntry",
                "title": "Edit"
            },
            {
                "command": "bookTreeView.deleteEntry",
                "title": "Delete"
            }
        ]
    },
    "devDependencies": {
        "@types/node": "^7.0.43",
        "typescript": "^2.6.1",
        "vscode": "^1.1.6"
    }
}
